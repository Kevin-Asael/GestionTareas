@model IEnumerable<GestionTareas.Models.Seguimiento>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.estadoAnterior)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.estadoNuevo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.tareaId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.usuarioId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.estadoAnterior)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.estadoNuevo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tareaId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.usuarioId)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.id })
            </td>
        </tr>
}
    </tbody>
</table>
